{
    "name": "hyperf/clickhouse",
    "description": "A clickhouse library for hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "clickhouse"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "smi2/phpclickhouse": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9",
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "suggest": {
        "psr/container": "Required to use ClickhouseFactory.",
        "hyperf/config": "Required to read clickhouse config."
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Clickhouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Clickhouse\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Clickhouse\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
